(ns arduino
  
  (:import [
	    [javax.comm SerialPort]])
  (:gen-class))


(defn get-port []
  (doto (new SerialPort)
    (.setSerialPortParameters 9600
			      SerialPort/DATABITS_8
			      SerialPort/STOPBITS_1
			      SerialPort/PARITY_NONE)))

(defn -main []
  (println "hello world"))

(-main)